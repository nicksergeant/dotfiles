[
  {
    "key": "ctrl+n",
    "command": "editor.action.marker.next",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+p",
    "command": "editor.action.marker.prev",
    "when": "editorFocus"
  },
  {
    "key": "f1",
    "command": "workbench.action.tasks.toggleProblems"
  },
  {
    "key": "f1",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "alt+f8",
    "command": "-editor.action.marker.next",
    "when": "editorFocus"
  },
  {
    "key": "shift+alt+f8",
    "command": "-editor.action.marker.prev",
    "when": "editorFocus"
  }
]
