{
  "editor.accessibilitySupport": "off",
  "editor.colorDecorators": false,
  "editor.cursorBlinking": "solid",
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.guides.indentation": false,
  "editor.hover.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineHeight": 1.2,
  "editor.lineNumbers": "off",
  "editor.matchBrackets": "never",
  "editor.occurrencesHighlight": "off",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.showFoldingControls": "never",
  "editor.tabSize": 2,
  "eslint.quiet": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github-enterprise.uri": "https://git.hubteam.com/",
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "makefile.configureOnOpen": false,
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.requireConfig": true,
  "search.followSymlinks": false,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontSize": 15,
  "typescript.autoClosingTags": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.includeAutomaticOptionalChainCompletions": false,
  "typescript.tsserver.log": "off",
  "typescript.tsserver.maxTsServerMemory": 16543,
  "typescript.tsserver.nodePath": "/Users/nsergeant/.bpm/packages/node-20-11-0/static-1.1/bin/node",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "start",
  "vim.hlsearch": true,
  "vim.matchpairs": "(:),{:},[:],<:>",
  "window.customTitleBarVisibility": "never",
  "window.nativeFullScreen": false,
  "workbench.colorTheme": "Badwolf",
  "workbench.editor.showTabs": "single",
  "workbench.hover.delay": 500,
  "workbench.statusBar.visible": false,
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": "explicit"
  },
  "files.associations": {
    "en.lyaml": "yaml"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "diffEditor.insertedTextBackground": "#00000099",
    "editor.wordHighlightBorder": "#ffdb02",
    "editor.findMatchHighlightBorder": "#ffdb02"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "support.type.property-name",
        "settings": {
          "foreground": "#FFFFFF"
        }
      }
    ]
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-l>"],
      "commands": [
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.log($1)$0"
          }
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["M"],
      "after": ["0"]
    },
    {
      "before": [",", "."],
      "commands": [
        {
          "command": "workbench.action.showAllSymbols"
        }
      ]
    },
    {
      "before": [",", "w"],
      "commands": [
        {
          "command": "editor.action.toggleWordWrap"
        }
      ]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["tab"],
      "after": ["%"]
    },
    {
      "before": [",", "a"],
      "commands": [
        {
          "command": "workbench.action.findInFiles"
        }
      ]
    },
    {
      "before": [",", "A"],
      "commands": [
        {
          "command": "editor.action.addSelectionToNextFindMatch"
        },
        {
          "command": "workbench.action.findInFiles"
        }
      ]
    },
    {
      "before": ["<C-j>"],
      "commands": [
        {
          "command": "workbench.action.navigateDown"
        }
      ]
    },
    {
      "before": ["<C-k>"],
      "commands": [
        {
          "command": "workbench.action.navigateUp"
        }
      ]
    },
    {
      "before": ["<C-h>"],
      "commands": [
        {
          "command": "workbench.action.navigateLeft"
        }
      ]
    },
    {
      "before": ["<C-l>"],
      "commands": [
        {
          "command": "workbench.action.navigateRight"
        }
      ]
    },
    {
      "before": ["g", "i"],
      "commands": [
        {
          "command": "editor.action.showHover"
        }
      ]
    },
    {
      "before": [",", ","],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": [",", "r"],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": [",", "v"],
      "commands": [
        { "command": "workbench.action.closeEditorsInOtherGroups" },
        {
          "command": "workbench.action.toggleZenMode"
        }
      ]
    },
    {
      "before": [",", "e", "v"],
      "commands": [
        {
          "command": "workbench.action.openSettingsJson"
        }
      ]
    },
    {
      "before": [",", "e", "k"],
      "commands": [
        {
          "command": "workbench.action.openGlobalKeybindings"
        }
      ]
    },
    {
      "before": [",", "e", "g"],
      "commands": [
        {
          "command": "gitlens.toggleFileBlame"
        }
      ]
    },
    {
      "before": [",", "f"],
      "commands": [
        {
          "command": "workbench.files.action.focusFilesExplorer"
        }
      ]
    },
    {
      "before": ["m", "v"],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": [",", "g"],
      "commands": [
        {
          "command": "issue.openGithubPermalink"
        }
      ]
    },
    {
      "before": [";"],
      "after": ["esc"],
      "commands": [
        {
          "command": "editor.action.formatDocument"
        }
      ]
    },
    {
      "before": [",", "c", ","],
      "after": ["esc"],
      "commands": [
        {
          "command": ":noh"
        }
      ]
    },
    {
      "before": [",", "c", "space"],
      "commands": [
        {
          "command": "editor.action.commentLine"
        }
      ]
    },
    {
      "before": ["esc"],
      "after": ["esc"],
      "commands": [
        {
          "command": ":noh"
        }
      ]
    },
    {
      "before": ["g", "o"],
      "after": ["g", "d"]
    },
    {
      "before": ["g", "s"],
      "after": ["*", "N"]
    },
    {
      "before": ["c", "s", "/"],
      "after": ["c", "g", "n"]
    },
    {
      "before": ["tab"],
      "after": ["%"]
    },
    {
      "before": ["W"],
      "after": ["v", "a", "t"]
    },
    {
      "before": ["g", "m"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
