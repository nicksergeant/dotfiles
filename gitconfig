  co = checkout
  merge = merge --no-ff
  pull = pull --rebase
  pushf = push --force-with-lease
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[core]
  excludesfile = ~/.gitignore
	hooksPath = /Users/nsergeant/.git_hooks
[hub]
  host = github.com
  protocol = https
	host = git.hubteam.com
