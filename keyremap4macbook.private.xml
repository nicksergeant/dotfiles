<?xml version="1.0"?>
<root>
    <appdef>
        <appname>FIREFOX</appname>
        <equal>org.mozilla.firefox</equal>
    </appdef>
    <appdef>
        <appname>CHROME</appname>
        <equal>com.google.Chrome</equal>
    </appdef>
    <appdef>
        <appname>SUBLIME</appname>
        <equal>com.sublimetext.2</equal>
    </appdef>
    <appdef>
        <appname>ITERM</appname>
        <equal>com.googlecode.iterm2</equal>
    </appdef>
    <appdef>
        <appname>LINKINUS</appname>
        <equal>net.conceited.Linkinus</equal>
    </appdef>
    <appdef>
        <appname>ITUNES</appname>
        <equal>com.apple.iTunes</equal>
    </appdef>
    <appdef>
        <appname>SPARROW</appname>
        <equal>com.sparrowmailapp.sparrow</equal>
    </appdef>
    <list> 
        <item> 
            <name>iTerm</name> 
            <identifier>private.iterm</identifier>
            <list> 
                <item>
                    <appendix>Cmd+Shift+Control+] for Option+Shift+A</appendix>
                    <autogen>--KeyToKey-- KeyCode::BRACKET_RIGHT, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L | ModifierFlag::CONTROL_L, KeyCode::A, ModifierFlag::OPTION_L</autogen>
                    <only>ITERM</only>
                </item>
                <item>
                    <appendix>Ctrl+I for Option+Shift+[</appendix>
                    <autogen>--KeyToKey-- KeyCode::BRACKET_LEFT, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L, KeyCode::BRACKET_LEFT, ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L</autogen>
                    <only>ITERM</only>
                </item>
                <item>
                    <appendix>Cmd+Shift+] for Option+Shift+]</appendix>
                    <autogen>--KeyToKey-- KeyCode::BRACKET_RIGHT, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L, KeyCode::BRACKET_RIGHT, ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L</autogen>
                    <only>ITERM</only>
                </item>
                <item>
                    <appendix>Cmd+Shift+L for Option+Shift+U</appendix>
                    <autogen>--KeyToKey-- KeyCode::L, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L, KeyCode::U, ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L</autogen>
                    <only>ITERM</only>
                </item>
                <item>
                    <appendix>Cmd+Shift+D for Option+Shift+D</appendix>
                    <autogen>--KeyToKey-- KeyCode::D, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L, KeyCode::D, ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L</autogen>
                    <only>ITERM</only>
                </item>
                <item>
                    <appendix>Cmd+[ for Option+/</appendix>
                    <autogen>--KeyToKey-- KeyCode::BRACKET_LEFT, ModifierFlag::COMMAND_L, KeyCode::SLASH, ModifierFlag::OPTION_L</autogen>
                    <only>ITERM</only>
                </item>
            </list>
        </item>
        <item> 
            <name>Linkinus</name> 
            <identifier>private.linkinus</identifier>
            <list> 
                <item>
                    <appendix>Cmd+Control+U for Page Up</appendix>
                    <autogen>--KeyToKey-- KeyCode::U, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L, KeyCode::PAGEUP</autogen>
                    <only>LINKINUS</only>
                </item>
                <item>
                    <appendix>Cmd+Control+D for Page Down</appendix>
                    <autogen>--KeyToKey-- KeyCode::D, ModifierFlag::COMMAND_L | ModifierFlag::CONTROL_L, KeyCode::PAGEDOWN</autogen>
                    <only>LINKINUS</only>
                </item>
            </list>
        </item>
        <item> 
            <name>iTunes</name> 
            <identifier>private.itunes</identifier>
            <list> 
                <item>
                    <appendix>Cmd+F for Search Music</appendix>
                    <autogen>--KeyToKey-- KeyCode::F, ModifierFlag::COMMAND_L, KeyCode::F, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L</autogen>
                    <only>ITUNES</only>
                </item>
            </list>
        </item>
    </list>
</root>
